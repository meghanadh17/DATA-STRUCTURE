#include <stdio.h>

int linearSearch(int arr[], int n, int key) {
    for (int i = 0; i < n; i++) {
        if (arr[i] == key) {
            return i;
        }
    }
    return -1;
}

int main() {
    int arr[] = {12, 45, 67, 89, 34, 27};
    int n = sizeof(arr) / sizeof(arr[0]);
    int key = 89;
    int result = linearSearch(arr, n, key);
    if (result != -1) {
        printf("Element found at index: %d", result);
    } else {
        printf("Element not found");
    }
    return 0;
}
