#include<stdio.h>
int main(){
    int a,r,sum=0;
    printf("Enter the number :");
    scanf("%d",&a);
    int b=a;
    while (a!=0){
        r=a%10;
        sum=sum*10+r;
        a=a/10;
		}
        if (sum==b)
            printf("\n%d is a palindrome number",b);
        else
            printf("%d is not a Palindrome number",b);

return 0;}
